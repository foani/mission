 {
   "name": "creata-mission-backend",
   "version": "1.0.0",
   "description": "CreataChain Telegram Mission Game Backend API Server",
   "main": "dist/index.js",
   "type": "module",
   "scripts": {
     "dev": "tsx watch src/index.ts",
     "build": "tsc",
     "start": "node dist/index.js",
     "start:dev": "tsx src/index.ts",
    "test": "vitest run",
    "test:watch": "vitest watch",
     "lint": "eslint src/**/*.ts",
     "lint:fix": "eslint src/**/*.ts --fix",
     "db:migrate": "prisma migrate dev",
     "db:generate": "prisma generate",
     "db:seed": "tsx prisma/seed.ts",
     "db:studio": "prisma studio",
     "docker:build": "docker build -t creata-mission-backend .",
     "docker:run": "docker run -p 3000:3000 creata-mission-backend"
   },
   "keywords": [
     "creatachain",
     "telegram",
     "miniapp",
     "blockchain",
     "game",
     "mission",
     "fastify",
     "prisma",
     "postgresql"
   ],
   "author": "CreataChain Team",
   "license": "MIT",
   "dependencies": {
     "@fastify/cors": "^8.4.2",
     "@fastify/swagger": "^8.13.0",
     "@fastify/swagger-ui": "^2.1.0",
     "@prisma/client": "^5.7.1",
     "dotenv": "^16.3.1",
     "ethers": "^6.9.0",
     "fastify": "^4.25.2",
     "jsonwebtoken": "^9.0.2",
     "zod": "^3.22.4"
   },
   "devDependencies": {
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.4",
     "@typescript-eslint/eslint-plugin": "^6.13.1",
     "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^8.54.0",
    "vitest": "^1.0.0",
    "prisma": "^5.7.1",
     "tsx": "^4.6.0",
     "typescript": "^5.3.2"
   },
   "engines": {
     "node": ">=18.0.0",
     "npm": ">=8.0.0"
   }
 }
